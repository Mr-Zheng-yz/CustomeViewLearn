<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MaterialEditText">
        <attr name="userFloatingLabel" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="DashboardProgressView">
        <attr name="progressColor" format="color"/>
        <attr name="progressTextColor" format="color"/>
        <attr name="circleBackgroundColor" format="color"/>
        <attr name="titleTextColor" format="color"/>
        <attr name="titleText" format="string"/>
    </declare-styleable>

    <declare-styleable name="RoundConstraintLayout">
        <attr name="cl_cornerRadius" format="dimension" />
        <attr name="cl_cornerRadius_leftTop" format="dimension" />
        <attr name="cl_cornerRadius_leftBottom" format="dimension" />
        <attr name="cl_cornerRadius_rightTop" format="dimension" />
        <attr name="cl_cornerRadius_rightBottom" format="dimension" />
        <attr name="cl_borderWidth" format="dimension" />
        <attr name="cl_borderColor" format="color" />
    </declare-styleable>

</resources>